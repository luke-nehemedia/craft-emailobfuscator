{
    "name": "lucasbares/craft-emailobfuscator",
    "description": "A simple plugin that adds a twig tag to obfuscate email addresses (by rot13) in text fields.",
    "type": "craft-plugin",
    "version": "2.2.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft emailobfuscator",
        "spam",
        "spam-protection",
        "email",
        "mail",
        "privacy",
        "robots",
        "obfuscator",
        "rot13"
    ],
    "support": {
        "docs": "https://github.com/luke-nehemedia/craft-emailobfuscator/blob/master/README.md",
        "issues": "https://github.com/luke-nehemedia/craft-emailobfuscator/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Bares",
            "homepage": "http://luke.nehemedia.de"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "lucasbares\\craftemailobfuscator\\": "src/"
        }
    },
    "extra": {
        "name": "Craft Emailobfuscator",
        "handle": "craft-emailobfuscator",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/luke-nehemedia/craft-emailobfuscator/master/CHANGELOG.md",
        "class": "lucasbares\\craftemailobfuscator\\CraftEmailobfuscator"
    }
}
